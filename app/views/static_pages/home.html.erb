<% if logged_in? %>
  <div class="row">
    <aside class="col-md-4">
      <%= render 'shared/home_page_users' %>
    </aside>
    <div class="col-md-8">
      <h3>Micropost Feed</h3>
      <%= render 'shared/feed' %>
    </div>
  </div>
<% else %>
  <div class = "center jumbotron">
    <%= render 'home_page' %>
  </div>
  
  <%= link_to image_tag("rails.png", alt:"Rails logo"), 'http://rubyonrails.org/' %>
  <%#= link_to image_tag("kitten.jpg", alt:"Cat logo") %>
<% end %>